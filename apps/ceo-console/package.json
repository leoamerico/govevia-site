{
  "name": "@govevia/ceo-console",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "smoke:brand": "node tools/smoke/smoke-brand-envneo.mjs",
    "smoke:rag": "node tools/smoke/smoke-rag-demo.mjs",
    "smoke:identity": "node tools/smoke/smoke-corporate-identity.mjs",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "PWDEBUG=1 playwright test",
    "demo:record": "playwright test --config=playwright.demo.config.ts --headed",
    "demo:gif": "node scripts/webm-to-gif.mjs",
    "demo:publish": "node scripts/demo-publish.mjs",
    "demo": "npm run demo:record && npm run demo:gif && npm run demo:publish"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "jose": "^5.10.0",
    "next": "14.2.35",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "ffmpeg-static": "^5.3.0",
    "typescript": "^5"
  }
}
